{
  "name": "muratec-install-logistics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bunx concurrently \"bunx convex dev\" \"bun run --bun next dev --turbo\"",
    "build": "bun run --bun next build",
    "start": "bun run --bun next start",
    "preview": "bun run build && bunx concurrently \"bunx convex dev\" \"bun run start\"",
    "lint": "bun run --bun eslint . --cache",
    "lint:fix": "bun run --bun eslint --fix .",
    "typecheck": "bun run --bun tsc --noEmit",
    "check": "bun run --bun eslint . --cache && tsc --noEmit",
    "format": "bun run --bun prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css}\" --cache -l",
    "format:check": "bun run --bun prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css}\" --cache",
    "import:prepare": "bun run scripts/csv-to-convex-jsonl.ts"
  },
